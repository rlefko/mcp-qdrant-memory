{
  "name": "@delorenj/mcp-qdrant-memory",
  "version": "0.2.5",
  "description": "MCP server with semantic scope implementation access and enhanced progressive disclosure for Claude Code memory.",
  "license": "MIT",
  "author": "Jarad DeLorenzo",
  "homepage": "https://github.com/delorenj/mcp-qdrant-memory",
  "bugs": "https://github.com//delorenj/mcp-qdrant-memory/issues",
  "type": "module",
  "bin": {
    "mcp-qdrant-memory": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "husky",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.1",
    "@qdrant/js-client-rest": "^1.13.0",
    "axios": "^1.8.1",
    "dotenv": "^16.3.1",
    "minimatch": "^9.0.0",
    "okapibm25": "^1.4.1",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/dotenv": "^8.2.0",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "shx": "^0.3.4",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.50.0",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
